<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
    <RootNamespace>TrackingMore_API</RootNamespace>
    <AssemblyName>TrackingMore-API</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageId>TrackingMore</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <Authors>TrackingMore Team</Authors>
    <Copyright>Copyright 2014-2023</Copyright>
    <Owners>TrackingMore</Owners>
    <Description>TrackingMore API, track your shipments</Description>
    <PackageTags>TrackingMore Track Tracking Courier Couriers</PackageTags>
    <PackageIconUrl>https://s.trackingmore.com/images/favicon.ico</PackageIconUrl>
    <PackageProjectUrl>https://www.trackingmore.com</PackageProjectUrl>
    <Title>TrackingMore</Title>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
